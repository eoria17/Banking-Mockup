@model Account
@{
    ViewData["Title"] = "Transfer";
}

<h1 class="display-4">Bank Transfer Account Number @Model.AccountNumber (@Model.AccountType)</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Transfer">
            <label asp-for="AccountNumber" class="control-label"></label>
        </form>
    </div>
</div>